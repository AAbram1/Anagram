function anagram(word1, word2) {
  // Code here!
  var out = false;
  i = 0;
  y = 0;
 let wd1 = word1; 
 let wd2 = word2;
  
  for(let value of wd1 ){
    console.log(value)
    console.log(wd2[i])
    if(value === wd2[i]){
